
config smartdns
	option enabled '1'
	option server_name 'indoor'
	option port '6051'
	option tcp_server '1'
	option ipv6_server '1'
	option dualstack_ip_selection '1'
	option prefetch_domain '0'
	option serve_expired '0'
	option redirect 'none'
	option cache_size '4194304'
	option rr_ttl_min '360'
	option rr_ttl_max '86400'
	option seconddns_server_group 'outdoor'
	option seconddns_enabled '1'
	option seconddns_port '6050'
	option seconddns_tcp_server '1'
	option seconddns_no_speed_check '1'
	option seconddns_no_rule_addr '0'
	option seconddns_no_rule_nameserver '0'
	option seconddns_no_rule_ipset '0'
	option seconddns_no_rule_soa '0'
	option seconddns_no_dualstack_selection '1'
	option seconddns_no_cache '1'
	option force_aaaa_soa '0'
	option coredump '0'

config server
	option type 'udp'
	option name 'Sichuan'
	option ip '61.139.2.69'
	option blacklist_ip '0'
	option server_group 'indoor'
	option enabled '0'
	option addition_arg '-exclude-default-group'

