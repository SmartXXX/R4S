	option type 'tls'
	option server_group 'outdoor'
	option blacklist_ip '0'
	option no_check_certificate '0'
	option name 'Google'
	option enabled '0'
	option ip '8.8.4.4'

config server
	option type 'tls'
	option server_group 'outdoor'
	option blacklist_ip '0'
	option no_check_certificate '0'
	option ip '1.1.1.1'
	option enabled '0'
